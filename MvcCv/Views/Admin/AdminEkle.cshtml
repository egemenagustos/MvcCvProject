@model MvcCv.Models.Entity.admin
@{
    ViewBag.Title = "AdminEkle";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}



<br />
<h4>Yeni Admin Ekleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Kullanıcı Adı")
    @Html.TextBoxFor(x => x.kullaniciAdi, new { @class = "form-control" })
    <br />

    @Html.Label("Şifreniz")
    @Html.TextBoxFor(x => x.sifre, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-dark">Ekle</button>
</form>


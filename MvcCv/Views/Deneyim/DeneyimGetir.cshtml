@model MvcCv.Models.Entity.deneyimlerim
@{
    ViewBag.Title = "DeneyimGetir";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}
<br />
<h4>Deneyim Güncelleme Sayfası</h4>
<br />
<form method="post">
    @Html.HiddenFor(x => x.id, new { @class = "form-control" })

    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })
    <br />

    @Html.Label("Alt-Başlık")
    @Html.TextBoxFor(x => x.altBaslik, new { @class = "form-control" })
    <br />

    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.tarih, new { @class = "form-control" })
    <br />

    @Html.Label("Başlık")
    @Html.TextAreaFor(x => x.aciklama, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-dark">Güncelle</button>
</form>
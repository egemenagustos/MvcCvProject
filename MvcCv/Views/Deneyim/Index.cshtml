@using MvcCv.Models.Entity
@model List<deneyimlerim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}
<br />
<h2>Deneyim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt-Başlık</th>
        <th>Açıklama</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.id</td>
                    <td>@item.Baslik</td>
                    <td>@item.altBaslik</td>
                    <td>@item.aciklama</td>
                    <td>@item.tarih</td>
                    <td><a href="/Deneyim/DeneyimSil/@item.id" class="btn btn-outline-danger">Sil</a></td>
                    <td><a href="/Deneyim/DeneyimGetir/@item.id" class="btn btn-outline-success">Güncelle</a></td>
                </tr>
            }
</table>
<a href="/Deneyim/DeneyimEkle" class="btn btn-outline-primary">Yeni Deneyim Ekle</a>

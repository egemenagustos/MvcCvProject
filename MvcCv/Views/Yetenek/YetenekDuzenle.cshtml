@model MvcCv.Models.Entity.yeteneklerim
@{
    ViewBag.Title = "YetenekDuzenle";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<br />
<h4>Yetenek Güncelleme Sayfası</h4>
<br />
<form method="post">
    @Html.HiddenFor(x => x.id, new { @class = "form-control" })

    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.yetenek, new { @class = "form-control" })
    <br />

    @Html.Label("Oran")
    @Html.TextBoxFor(x => x.oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-dark">Güncelle</button>
</form>

